<component name="mailchimp-lists" type="synapse/template">
    <subComponents>
        <component name="createList">
            <file>createList.xml</file>
            <description>Create a new list.</description>
        </component>
        <component name="listLists">
            <file>listLists.xml</file>
            <description>Get information about all lists.</description>
        </component>
        <component name="getList">
            <file>getList.xml</file>
            <description>Get information about a specific list.</description>
        </component>
        <component name="deleteList">
            <file>deleteList.xml</file>
            <description>Delete a list.</description>
        </component>
        <component name="listListAbuseReports">
            <file>listListAbuseReports.xml</file>
            <description>Get information about abuse reports.</description>
        </component>
        <component name="getListActivity">
            <file>getListActivity.xml</file>
            <description>Get recent list activity.</description>
        </component>
        <component name="subscribeOrUnsubscribeListMembers">
            <file>subscribeOrUnsubscribeListMembers.xml</file>
            <description>Batch subscribe or unsubscribe list members.</description>
        </component>
        <component name="updateList">
            <file>updateList.xml</file>
            <description>Update a specific list.</description>
        </component>
    </subComponents>
</component>